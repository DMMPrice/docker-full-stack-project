<!DOCTYPE html>
<html>
  <head>
    <title>Uploaded Data</title>
    <link rel="stylesheet" type="text/css" href="/styles.css" />
  </head>
  <body>
    <div class="container">
      <h1>Uploaded Data</h1>
      <table class="data-table">
        <thead>
          <tr>
            <% if (data.length > 0) { %> <% Object.keys(data[0]).forEach(key =>
            { %>
            <th><%= key %></th>
            <% }) %> <% } %>
          </tr>
        </thead>
        <tbody>
          <% data.forEach(row => { %>
          <tr>
            <% Object.values(row).forEach(value => { %>
            <td><%= value %></td>
            <% }) %>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </body>
</html>
